.orderNo=orderNo;\n        this.confirmOrder.payStatus=payStatus;\n        this.accountPubService.confirmOrderZ(this.confirmOrder).subscribe(success => {\n            console.log(success)\n\n        },error => {\n\n        })\n    }\n  Order(flag){\n        console.log(flag);\n     console.log(\"order点击了\");\n      if(flag=='0'){  //取消\n          this.orderNo =this.order.orderno;\n          this.payStatus =  false;\n          this.confirmOrderCon(this.orderNo,this.payStatus);\n          this.navCtrl.parent.select(0);\n      }else if(flag=='1'){  //确定\n          // this.browserService.open(success.qr_code);\n          let alert = this.alertCtrl.create({\n              title: this.translateObj.warning,\n              subTitle: this.translateObj.goPay,\n              buttons: [\n                  {\n                      text: this.translateObj.goPay,\n                      handler: data => {\n                          console.log('Saved clicked');\n                          if(flag.qrCode){ //二维码\n                              this.browserService.open(flag.qr_code);\n                          }\n                          //弹出\n                          setTimeout(()=>{\n                              setTimeout(() => {\n                                  this.showConfirm(flag);\n                              }, 2000);\n                          })\n                          // this.browserService.open(success.qr_code);\n\n\n                      }\n                  }\n              ]\n          });\n          alert.present();\n      }\n      // this.navCtrl.push('TopupPage',{flag:'order',order:this.order})\n\n  }\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/financial/flowdetail/bill-detail/bill-detail.ts"],"sourceRoot":""}