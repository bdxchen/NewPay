var NumberUtils = {
    toImpliedDecimal: function toImpliedDecimal(number, precision) {
        if (typeof number === "number") {
            __WEBPACK_IMPORTED_MODULE_0_assert___default()(number <= 9007199254740991, "overflow");
            number = "" + number;
